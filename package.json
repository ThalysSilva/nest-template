  "scripts": {
    "build": "echo 'Iniciando build...' && npx prisma generate && nest build",
    "build-vercel": "echo 'Rodando o comando build-vercel' && yarn build",
    "postinstall": "echo 'Rodando o comando postinstall' && prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:db-pull": "npx prisma db pull",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
